// Clear the workspace
clc; clear;

// Define matrix A
A = [1 2 3; 4 5 6; 2 4 1];

// Calculate and display the inverse of A
disp('Inverse of A:');
disp(inv(A));

// Calculate and display the determinant of A
disp('Determinant of A:');
disp(det(A));

// Calculate and display the trace of A
disp('Trace of A:');
disp(trace(A));

// Calculate and display the rank of A
disp('Rank of A:');
disp(rank(A));
